/* components/Songs/Songs.wxss */
.songList{
    padding: 10px 5px;
    width: 100%;
    height: 345px;
}
.song{
    padding: 0px 20px 0px 10px;
    box-sizing: border-box;
    height: 50px;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.songName{
    width: 100%;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}
.songArter{
    font-size: 12px;
}
image{
    width: 20px;
    height: 20px;
}